<meta name="theme-color" content="#fdb51f">        <script>
    (function(win) {
        'use strict';

        var doc = win.document;
        var supported = "localStorage" in win && null !== win.localStorage

        // Wait until body tag is defined. This is to prevent blocking in IE11
        function ready(cb) {
            doc.body ? cb() : setTimeout(function() { ready(cb) });
        }

        var loadCSS = function(href) {
            // Create local storage key
            var key = (function(str) {
                var start = str.lastIndexOf('/') + 1;
                var end = str.lastIndexOf('.');
                end = end === -1 ? str.length : end;

                return str.substring(start, end) + 'v2';
            })(href);

            // Create style element
            var elem = doc.createElement('style');
            elem.rel = 'stylesheet';

            // Find last element
            var refs = (doc.body || doc.getElementsByTagName("head")[0]).childNodes;
            var ref = refs[refs.length - 1];

            // Define variable placeholder
            var font;
            if(supported && (font = localStorage.getItem(key))) {
                elem.textContent = font;

                // Add element to DOM
                ref.parentNode.insertBefore(elem, ref);
            }
            else {
                // Open new XMLHttpRequest
                var request = new XMLHttpRequest();
                request.open('GET', href, true);

                // State change callback
                request.onreadystatechange = function() {
                    if(this.readyState === 4) {
                        if(this.status >= 200 && this.status < 400) {
                            // Get response
                            font = this.responseText;

                            // Save in local storage
                            if(supported) localStorage.setItem(key, font);

                            // Set text content of style element
                            elem.textContent = font;

                            // Add element to DOM
                            ref.parentNode.insertBefore(elem, ref);
                        }
                    }
                };

                // Send request
                request.send();
            }


            return elem;
        }

        // Get browser user agent
        var ua = window.navigator.userAgent;

        // Load ttf
        if(ua.indexOf( "Android 4." ) > -1 && ua.indexOf( "like Gecko" ) > -1 && ua.indexOf( "Chrome" ) === -1 ) {
            loadCSS('/bundles/tvnucore/css/proxima_nova-ttf.css');
        }
        // Default to woff
        else {
            loadCSS('/bundles/tvnucore/css/proxima_nova-woff.css');
        }

    })(typeof global !== "undefined" ? global : this);
</script>
    
		
